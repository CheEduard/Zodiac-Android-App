<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.compatibility.CompatibilityFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/header_container"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_60sdp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView_birthdate_psychomatrix"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16sdp"
            android:fontFamily="@font/jost_bold"
            android:textSize="@dimen/_16ssp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/choose_date_btn_psychomatrix"
            android:layout_width="@dimen/_32sdp"
            android:layout_height="@dimen/_32sdp"
            android:layout_marginEnd="@dimen/_16sdp"
            android:background="@android:color/transparent"
            app:icon="@drawable/btn_choose_date"
            app:iconGravity="textStart"
            app:iconPadding="0dp"
            app:iconTint="?attr/colorPrimary"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_container">

        <TextView
            android:id="@+id/textView_1_temper"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center"
            android:textAlignment="center"
            />

        <TextView
            android:id="@+id/textView_2_charisma"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_1_temper"
            android:gravity="center"
            android:textAlignment="center"
            />

        <TextView
            android:id="@+id/textView_3_knowledge"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_2_charisma"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_4_health"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toEndOf="@+id/textView_1_temper"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_5_logic"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toEndOf="@+id/textView_2_charisma"
            app:layout_constraintTop_toBottomOf="@+id/textView_4_health"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_6_mastery"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toEndOf="@+id/textView_3_knowledge"
            app:layout_constraintTop_toBottomOf="@+id/textView_5_logic"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_7_luck"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView_4_health"
            app:layout_constraintTop_toTopOf="parent"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_8_sense_of_duty"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView_5_logic"
            app:layout_constraintTop_toBottomOf="@+id/textView_7_luck"
            android:gravity="center"
            android:textAlignment="center"/>

        <TextView
            android:id="@+id/textView_9_intelligence"
            android:layout_margin="@dimen/_10sdp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/img_psychomatrix_square"
            app:layout_constraintStart_toEndOf="@+id/textView_6_mastery"
            app:layout_constraintTop_toBottomOf="@+id/textView_8_sense_of_duty"
            android:gravity="center"
            android:textAlignment="center"/>
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>